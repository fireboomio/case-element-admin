mutation UpdateOnePost($id: Int!, $title: String, $poster: String, $publishedAt: DateTime, $User: main_UserUpdateOneRequiredWithoutPostNestedInput) {
  data: main_updateOnePost(
    data: {title: {set: $title}, poster: {set: $poster}, publishedAt: {set: $publishedAt}, User: $User}
    where: {id: $id}
  ) {
    
    id
    title
    poster
    publishedAt
    authorId
    User {
      name
      id
      avatarUrl
    }
  }
}
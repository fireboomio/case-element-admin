{
  "disableForceHttpsRedirects": true,
  "authorizedRedirectUris": [
    "http://localhost:9123/#/workbench/userInfo",
    "http://localhost:9123/#/workbench/rapi/loginBack",
    "http://localhost:3000/",
    "http://localhost:3000/#/dashboard"
  ],
  "configureWunderGraphApplication": {
    "security": {
      "allowedHostsEnabled": true,
      "enableGraphQLEndpoint": true,
      "allowedHosts": [],
      "enableCSRF": true
    },
    "cors": {
      "allowedOriginsEnabled": false,
      "allowedOrigins": [
        "http://localhost:3000"
      ],
      "allowedMethods": [
        "GET",
        "POST"
      ],
      "allowedHeadersEnabled": true,
      "allowedHeaders": [
        "*"
      ],
      "exposedHeaders": [
        "*"
      ],
      "exposedHeadersEnabled": true,
      "maxAge": 120,
      "allowCredentials": true
    }
  }
}